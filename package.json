{
  "name": "libzt",
  "version": "0.1.5",
  "description": "Idiomatic node bindings for libzt.",
  "keywords": [
    "zerotier",
    "libzt",
    "p2p",
    "vpn"
  ],
  "binary": {
    "napi_versions": [
      5
    ]
  },
  "main": "dist/index.js",
  "scripts": {
    "test": "node dist/test/test-load-lib.js",
    "install": "pkg-prebuilds-verify ./dist/binding-options.js || npm run compile",
    "compile": "cmake-js build -p 8",
    "recompile": "cmake-js rebuild -p 8",
    "build": "rimraf dist && tsc",
    "prettier": "npx prettier . --write"
  },
  "license": "ISC/BSL",
  "dependencies": {
    "cmake-js": "^7.3.0",
    "node-addon-api": "^8.0.0",
    "pkg-prebuilds": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.8",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.5"
  },
  "files": [
    "prebuilds",
    "dist"
  ]
}
